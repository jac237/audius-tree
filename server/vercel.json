{
  "version": 2,
  "name": "audius-tree-api",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "alias": [
    "audius-tree-api"
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "1337",
    "CORS_ORIGIN": ""
  }
}